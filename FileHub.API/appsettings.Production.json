{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      }
    }
  },
  "AllowedHosts": "*",
  "Cache": {
    "Host": "santaimvprediscache-hkpg4d.serverless.apse1.cache.amazonaws.com:6379",
    "Username": "",
    "Password": "",
    "ConnectTimeout": 30,
    "SyncTimeout": 30,
    "ReconnectRetryPolicy": 3,
    "CacheLifeTime": 20
  },
  "RateLimiter": {
    "EnableRateLimiting": true,
    "StackBlockedRequests": true,
    "RealIPHeader": "X-Real-IP",
    "ClientIdHeader": "X-Client-ID",
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "5s",
        "Limit": 10
      },
      {
        "Endpoint": "*",
        "Period": "1h",
        "Limit": 1000
      }
    ]
  },
  "Idempotency": {
    "IsActive": true,
    "TTL": 86400
  },
  "Jwt": {
    "Issuer": "identity.santaimvp.com",
    "Audience": "santaimvp.com",
    "TotalSecondsAccessTokenLifetime": 3600,
    "TotalDaysRefreshTokenLifetime": 30
  },
  "Storage": {
    "BucketPrivate": "santaitechnology-mvp-private-resource",
    "BucketPublic": "santaitechnology-mvp-public-resource",
    "CdnServiceUrl": "localhost:9000",
    "UseMinio": false,
    "MultipartBodyLengthLimit": 10485760
  },
  "AWS": {
    "AccessID": "",
    "SecretKey": "",
    "Region": ""
  }
}