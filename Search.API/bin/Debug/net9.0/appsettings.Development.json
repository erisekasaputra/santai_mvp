{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },  
  "Cache": {
    "Host": "localhost:6379",
    "Username": "",
    "Password": "",
    "ConnectTimeout": 10,
    "SyncTimeout": 10,
    "ReconnectRetryPolicy": 3,
    "CacheLifeTime": 20
  }, 
  "RateLimiter": {
    "EnableRateLimiting": true,
    "StackBlockedRequests": true,
    "RealIPHeader": "X-Real-IP",
    "ClientIdHeader": "X-Client-ID",
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "5s",
        "Limit": 10
      },
      {
        "Endpoint": "*",
        "Period": "1h",
        "Limit": 1000
      }
    ]
  }, 
  "Jwt": {
    "Issuer": "identity.santaimvp.com",
    "Audience": "santaimvp.com",
    "TotalSecondsAccessTokenLifetime": 3600,
    "TotalDaysRefreshTokenLifetime": 30
  }, 
  "Elasticsearch": {
    "Addresses": [
      "http://localhost:9200/"
    ]
  }
}
